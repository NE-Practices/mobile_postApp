{
  "expo": {
    "name": "Mobile Template",
    "slug": "Mobile Template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/imgs/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "eva",
    "splash": {
      "image": "./src/assets/images/imgs/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/imgs/logo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
